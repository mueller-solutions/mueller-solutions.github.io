{
  "name": "mueller-solutions-github-io",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "clean": "rm -rf dist/ && rm -rf public/admin/ && rm -rf coverage/",
    "dev": "tinacms dev -c \"astro dev --host\"",
    "start": "astro dev",
    "build": "pnpm clean && astro check && tinacms build && astro build",
    "build:local": "pnpm clean && astro check && tinacms build --local -c \"astro build\"",
    "preview": "astro preview",
    "astro": "astro",
    "format": "prettier --write \"**/*.{ts,js,astro}\"",
    "lint:styles": "stylelint \"src/**/*.css\"",
    "lint:astro": "astro check",
    "lint:ci": "pnpm lint:styles && pnpm lint:astro",
    "lint:staged": "lint-staged -q",
    "test:unit": "vitest",
    "test:unit:coverage": "vitest run --coverage",
    "prepare": "husky"
  },
  "lint-staged": {
    "**/*.{ts,js,astro}": [
      "prettier --write"
    ],
    "**/*.css": [
      "stylelint --fix"
    ]
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/netlify": "^6.0.0",
    "@astrojs/node": "^9.0.0",
    "@astrojs/partytown": "^2.1.2",
    "@astrojs/rss": "^4.0.10",
    "@frontendista/astro-html-minify": "^2.0.0",
    "@fullhuman/postcss-purgecss": "^7.0.2",
    "astro": "^5.0.4",
    "astro-imagetools": "^0.9.0",
    "astro-useragent": "^5.0.0",
    "astrojs-service-worker": "^2.0.0",
    "autoprefixer": "^10.4.20",
    "axios": "^1.7.9",
    "cssnano": "^7.0.6",
    "cssnano-preset-advanced": "^7.0.6",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "dotenv-expand": "^12.0.1",
    "jsonwebtoken": "^9.0.2",
    "postcss-import": "^16.1.0",
    "postcss-mixins": "^11.0.3",
    "postcss-nested": "^7.0.2",
    "postcss-preset-env": "^10.1.1",
    "purgecss": "^7.0.2",
    "sharp": "^0.33.5",
    "slugify": "^1.6.6",
    "tinacms": "^2.5.1",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "devDependencies": {
    "@tinacms/cli": "^1.6.13",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.8.7",
    "@types/wpapi": "^1.1.4",
    "@vitest/coverage-v8": "2.1.8",
    "husky": "9.1.7",
    "lint-staged": "^15.2.10",
    "prettier": "^3.4.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-organize-imports": "^4.1.0",
    "stylelint": "^16.11.0",
    "stylelint-config-standard": "^36.0.1"
  },
  "browserslist": "> 0.5%, last 2 versions, not ie <= 11, not ie_mob <= 11",
  "packageManager": "pnpm@9.12.3+sha512.cce0f9de9c5a7c95bef944169cc5dfe8741abfb145078c0d508b868056848a87c81e626246cb60967cbd7fd29a6c062ef73ff840d96b3c86c40ac92cf4a813ee"
}
